@page
@model Hephaestus_Project.Pages.MyEQ.Pages_MyEQ_Index
@{
}

<div class="text-center">
    <h1 class="display-4">Users</h1>
</div>
<a class="btn btn-primary btn-sm" href="/Users/Create">New User</a>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Weapon</th>
            <th>Serial</th>
            <th>Type</th>
            <th>InMaintance</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListMyEQ)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Weapon</td>
                <td>@item.Serial</td>
                <td>@item.Type</td>
                <td>@item.InMaintance</td>
            </tr>
        }
    </tbody>
</table>
